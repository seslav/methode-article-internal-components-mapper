<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE doc SYSTEM "/SysConfig/Rules/ftpsi.dtd">
<doc>
    <lead>
        {{#promotioanlTitleVariant}}
        <web-index-headline-variant>
            <ln>{{promotioanlTitleVariant}}</ln>
        </web-index-headline-variant>
        {{/promotioanlTitleVariant}}
        {{#promotioanlStandfirstVariant}}
        <web-stand-first-variant>
            <p>{{promotioanlStandfirstVariant}}</p>
        </web-stand-first-variant>
        {{/promotioanlStandfirstVariant}}
        {{#skyboxHeadline}}
        <lead-headline>
            <skybox-headline>
                {{skyboxHeadline}}
            </skybox-headline>
        </lead-headline>
        {{/skyboxHeadline}}
        {{#leadImageSet}}
        <lead-image-set>
                   <lead-image-square
                                fileref="/FT/Graphics/Online/Z_Undefined/2017/01/lalaland_square.jpg?uuid={{squareImageUUID}}"
                                xtransform=" scale(0.0279479 0.0279479)" tmx="2612 2612 73 73"/>
                        <lead-image-standard
                                fileref="/FT/Graphics/Online/Z_Undefined/2017/01/lalaland_original.jpg?uuid={{standardImageUUID}}"
                                xtransform=" scale(0.0279479 0.0279479)" tmx="4645 2612 129 73"/>
                        <lead-image-wide
                                fileref="/FT/Graphics/Online/Z_Undefined/2017/01/lalaland_letterbox.jpg?uuid={{wideImageUUID}}"
                                xtransform=" scale(0.0471576 0.0471576)" tmx="4645 1548 219 73"/>
        </lead-image-set>
        {{/leadImageSet}}
        <lead-components>
            {{#contentPackage}}
            <content-package design-theme="{{oldDesignTheme}}" sequence="{{tableOfContentsSequence}}" label="{{tableOfContentsLabelType}}">
                {{#contentPackageNext}}
                <content-package-next>{{contentPackageNext}}</content-package-next>
                {{/contentPackageNext}}
            </content-package>
            {{/contentPackage}}
            {{#topper}}
            <topper background-colour="{{topperBackgroundColour}}" layout="{{topperLayout}}">
                <topper-headline>
                    <ln>{{topperHeadline}}</ln>
                </topper-headline>
                <topper-standfirst>
                    <p>{{topperStandfirst}}</p>
                </topper-standfirst>
            </topper>
            {{/topper}}
            {{#summary}}
            <lead-summary display-position="{{displayPosition}}">
                <p>Latest government paper aims to tackle one of thorniest issues of departure with <b>bold text</b> and <i>italics</i> too. Here?s a<a href="http://www.ft.com">hyperlink</a>as well.</p>
                <ul>
                   <li>?Strong? case for frictionless Irish border after Brexit</li>
                   <li>
                      <a href="https://www.ft.com/content/3e466f6b-f030-347f-9c29-ad0bf7e6f094">Pound stumbles to new low against euro (ext link)</a>
                   </li>
                   <li>
                      <a dtxInsert="Summary Link" href="/FT/Content/World%20News/Stories/Live/Brexit%20papers.xml?uuid=ef943cc2-8266-11e7-b1b5-d77fe0865278">Britain?s Brexit papers aim to fast-track customs talks (int link)</a>
                   </li>
                </ul>
            </lead-summary>
            {{/summary}}
        </lead-components>
    </lead>
    <story>
        <text>
            <body>
                <p>random text for now</p>
                {{#imageSetID}}
                <image-set id="{{imageSetID}}">
                    <image-small id="U11603551664670Dp" fileref="/FT/Content/Companies/Stories/Live/GB/image%20SET/rj/New07.jpg?uuid=069ece4e-2a5d-11e7-9b32-1fc36bdf8f7d" xtransform=" scale(0.0729167 0.0729167)" tmx="2048 1152 71 84"/>
                    <image-medium id="U11603551664670MgH" fileref="/FT/Content/Companies/Stories/Live/GB/image%20SET/rj/New06.jpg?uuid=077aa180-2a5d-11e7-9b32-1fc36bdf8f7d" xtransform=" scale(0.0842014 0.0842014)" tmx="2048 1152 150 97"/>
                    <image-large id="U11603551664670OGB" fileref="/FT/Content/Companies/Stories/Live/GB/image%20SET/rj/New05.jpg?uuid=0848103e-2a5d-11e7-9b32-1fc36bdf8f7d" xtransform=" scale(0.0976563 0.0976563)" tmx="2048 1152 200 87"/>
                </image-set>
                {{/imageSetID}}
            </body>
        </text>
    </story>
</doc>